menu title FLISoL CABA PXE Server
prompt 0
timeout 0
default vesamenu.c32
menu include pxelinux.cfg/graphics.conf

label debian
    menu label Debian
    kernel vesamenu.c32
    append pxelinux.cfg/debian.conf

menu separator


label ubuntu
    menu label Ubuntu
    kernel vesamenu.c32
    append pxelinux.cfg/ubuntu.conf

menu separator

label tools
    menu label Tools
    kernel vesamenu.c32
    append pxelinux.cfg/tools.conf

menu separator

label localboot
        menu label Arranque local (HDD/SDD)
        localboot 0
